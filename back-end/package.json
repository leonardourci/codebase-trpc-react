{
  "name": "codebase-back-end",
  "version": "1.0.0",
  "description": "Codebase back-end",
  "scripts": {
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node dist/server.js",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "dev": "NODE_ENV=development npx nodemon --watch 'src/**/*.ts' --exec 'npx ts-node' src/server.ts",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker compose -f docker-compose.test.yml down",
    "test:db:logs": "docker compose -f docker-compose.test.yml logs -f postgres-test",
    "test:integration": "npm run test:db:up && npm test && npm run test:db:down",
    "db:migrate": "NODE_ENV=development npx knex migrate:latest --knexfile src/database/knexfile.ts",
    "db:migrate:test": "NODE_ENV=test npx knex migrate:latest --knexfile src/database/knexfile.ts",
    "db:rollback": "NODE_ENV=development npx knex migrate:rollback --knexfile src/database/knexfile.ts",
    "db:seed": "NODE_ENV=development npx knex seed:run --knexfile src/database/knexfile.ts"
  },
  "keywords": [],
  "author": "Leonardo Guimar√£es Urci <https://github.com/leonardourci>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/knex": "^0.15.2",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@trpc/client": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "stripe": "^20.1.0",
    "zod": "^4.3.5"
  }
}